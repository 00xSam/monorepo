{
  "name": "meteora-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:sdk": "turbo build --filter=@meteora-ag/vault-sdk",
    "dev": "npx concurrently 'turbo dev' 'npx turbowatch ./turbowatch.ts'",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "turbowatch": "^2.29.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
